<p <%= summonerName === e.name ? 'id=searchedSummoner' : '' %>>
  <a href="/<%= locale %>/player/<%= e.name %>"><img src="/ddragonData/img/profileicon/<%= e.iconId %>.png" 
  width="50px" height="50px"> </a>


  <span style="color: <%= summonerName === e.name ? 'red' : 'black' %>;
        font-weight:bold"><%= i %> -</span>
  <span style="color: <%= summonerName === e.name ? 'red; font-weight: bold;' : 'black' %>;">
   <%= e.name %> - <%= leaguejs.rank2string(e.rank, __) %>
 </span>
</p>


<% if (summonerName === e.name) { %>
<script type="text/javascript">
  document.getElementById('searchedSummoner').scrollIntoView();
</script>
<% } %>